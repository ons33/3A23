

<table>
    <tr>
        <th>ID</th>
        <th>User Name</th>
        <th>Email</th>
        
    </tr>
    {% for auth in authors %}
        <tr>
            <td>{{auth.id}}</td>
            <td>{{auth.username}}</td>
            <td>{{auth.email}}</td>
            <td>
                <a href="{{ path('app_delete', {'id': auth.id}) }}">Delete</a>
                </td>
        </tr>
    {% endfor %}
</table>